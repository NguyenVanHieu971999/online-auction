  <div class="container-fluid text-lg p-lg-5">
    <div class="row py-2">
      <div class="col-md-3 pl-5">
        <div class="card h-100 shadow">
          {{> home/side }}
        </div>
      </div>
      <div class="col-md-9">
        <div class="card h-100 shadow">
          {{#each this.product}}
